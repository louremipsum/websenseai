{
  "manifest_version": 3,
  "name": "websenseai",
  "description": "Know what is happening on your screen using Chrome's built-in AI",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "images/logo16.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  }
}
